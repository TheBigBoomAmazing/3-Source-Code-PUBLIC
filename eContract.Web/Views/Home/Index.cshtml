
@{ 
    var currentUser = (eContract.BusinessService.SystemManagement.Domain.UserDomain)eContract.Common.WebUtils.WebCaching.CurrentUserDomain;
    var goodsList = ViewBag.goods as List<eContract.Common.Entity.LubrFinancialGoodsEntity>;

}
<body>
    <div id="myCarousel" class="carousel slide" style="margin-top:10px;height:260px">
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/UI/bootstrap-2.3.2/docs/assets/img/examples/slide-01.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Example headline.</h1>
                        <p class="lead">Cras justo odio, dapibus ac facilisis in, egestas eget quam. Donec id elit non mi porta gravida at eget metus. Nullam id dolor id nibh ultricies vehicula ut id elit.</p>
                        <a class="btn btn-large btn-primary" href="#">Sign up today</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="~/UI/bootstrap-2.3.2/docs/assets/img/examples/slide-02.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Another example headline.</h1>
                        <p class="lead">Cras justo odio, dapibus ac facilisis in, egestas eget quam. Donec id elit non mi porta gravida at eget metus. Nullam id dolor id nibh ultricies vehicula ut id elit.</p>
                        <a class="btn btn-large btn-primary" href="#">Learn more</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="~/UI/bootstrap-2.3.2/docs/assets/img/examples/slide-03.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>One more for good measure.</h1>
                        <p class="lead">Cras justo odio, dapibus ac facilisis in, egestas eget quam. Donec id elit non mi porta gravida at eget metus. Nullam id dolor id nibh ultricies vehicula ut id elit.</p>
                        <a class="btn btn-large btn-primary" href="#">Browse gallery</a>
                    </div>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div><!-- /.carousel -->
    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">
        <!-- Three columns of text below the carousel -->
        <div class="row">
            <div class="span3">
                <img class="img-circle" src="~/UI/INSPINIA/img/a1.jpg" />
                <h2>安全</h2>
                <p>我们为您提供全方位安全的账户保护.</p>
                <p><a class="btn" href="#">View details &raquo;</a></p>
            </div>
            <div class="span3">
                <img class="img-circle" src="~/UI/INSPINIA/img/a6.jpg" />
                <h2>灵活</h2>
                <p>更具您的实际情况提供灵活的产品.</p>
                <p><a class="btn" href="#">View details &raquo;</a></p>
            </div>
            <div class="span3">
                <img class="img-circle" src="~/UI/INSPINIA/img/a3.jpg" />
                <h2>稳定</h2>
                <p>最大限度的保证你的收益.</p>
                <p><a class="btn" href="#">View details &raquo;</a></p>
            </div>
            <div class="span3">
                <img class="img-circle" src="~/UI/INSPINIA/img/a4.jpg" />
                <h2>可靠</h2>
                <p>这个不知道写什么了.</p>
                <p><a class="btn" href="#">View details &raquo;</a></p>
            </div>
        </div><!-- /.row -->
        <!-- START THE FEATURETTES -->
        <hr class="featurette-divider">
        <div class="container-fluid">
            <div class="row-fluid">
                @*<div class="span">
                    基础数据
                </div>*@
                <div class="span12">
                    <div class="span3">
                        <span style="color: #999999;line-height: 200%;">累计投资：</span>
                        <span style="font-size: 20px;color:red">2143254</span>
                        <span style="color: #999999;line-height: 200%;">亿元</span>
                    </div>
                    <div class="span3">
                        <span style="color: #999999;line-height: 200%;">累计赚取：</span>
                        <span style="font-size: 20px;color:red">3470</span>
                        <span style="color: #999999;line-height: 200%;">万元</span>

                    </div>
                    <div class="span3">
                        <span style="color: #999999;line-height: 200%;">昨日交易：</span>
                        <span style="font-size: 20px;color:red">2143</span>
                        <span style="color: #999999;line-height: 200%;">万元</span>
                    </div>
                    <div class="span3">
                        <span style="color: #999999;line-height: 200%;">稳定运行：</span>
                        <span style="font-size: 20px;color:red">450</span>
                        <span style="color: #999999;line-height: 200%;">天</span>

                    </div>
                </div>
            </div>
            <hr class="featurette-divider">
            <div class="row-fluid">
                <div class="span2" rowspan="3" style="height:180px;background-color:aqua">
                    产品
                </div>
                <div class="span10">
                    @foreach (var good in goodsList)
                    {
                        <div class="row-fluid" style="margin-top:20px">
                            <div class="span3">
                                <span style="font-size: 20px;color:red">@good.annualrate.ToString()</span>%<br>
                                <span style="color: #999999;line-height: 200%;">历史平均年化利率</span>
                            </div>
                            <div class="span3">
                                <span style="font-size: 20px;">@good.period.ToString()</span>天<br>
                                <span style="color: #999999;line-height: 200%;">借贷期限</span>

                            </div>
                            <div class="span3">
                                <span style="font-size: 20px;">@good.principal.ToString()</span>元<br>
                                <span style="color: #999999;line-height: 200%;">投资起点</span>
                            </div>
                            <div class="span3">
                                <a class="btn btn-large  btn-primary" type="button" href="@Url.Content("~/LUBR/ProductDetails?id='"+good.goodsid+"'")">了解详情</a>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
        <hr class="featurette-divider">
        <div class="container-fluid">
            <div class="row-fluid">
                <div class="span12">
                    合作伙伴
                </div>
            </div>
            <div class="row-fluid">
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/1.png" />
                </div>
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/10.png" />
                </div>
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/3.png" />
                </div>
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/2.png" />
                </div>
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/4.png" />
                </div>
                <div class="span2">
                    <img src="~/UI/INSPINIA/img/9.png" />
                </div>
            </div>
        </div>
        <hr class="featurette-divider">
        <!-- /END THE FEATURETTES -->
        <!-- FOOTER -->
        <footer>
            <p class="pull-right"><a href="#">回顶</a></p>
            @*<p>&copy; 2013 Company, Inc. &middot; <a href="#">Privacy</a> &middot; <a href="#">Terms</a></p>*@
        </footer>
    </div><!-- /.container -->
    <!-- Le javascript
    ================================================== -->
    <script src="~/UI/bootstrap-2.3.2/docs/assets/js/holder/holder.js"></script>
</body>



@section footer{
    <style type="text/css">
        body {
            margin: 0px;
        }

        .impowerBox {
            width: 196px;
        }

            .impowerBox .qrcode {
                width: 196px;
                margin-top: 0px;
            }

            .impowerBox .title {
                display: none;
            }

            .impowerBox .info {
                display: none;
            }

        .login-wrapper .login-qrcode {
            border: none;
        }
    </style>
    <script type="text/javascript">
    
    </script>
}
